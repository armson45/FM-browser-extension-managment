{
  "compilerOptions": {
    "types": [
      "astro/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@images/*": [
        "./public/assets/images/*"
      ],
      "@style/*": [
        "./src/assets/css/*"
      ],
    },
  },
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
}